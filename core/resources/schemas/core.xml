      <column columnName="ModifiedBy">
        <columnTitle>Modified By</columnTitle>
        <inputType>text</inputType>
        <inputLength>10</inputLength>
        <isReadOnly>true</isReadOnly>
        <fk>
          <fkColumnName>UserId</fkColumnName>
          <fkTable>Users</fkTable>
