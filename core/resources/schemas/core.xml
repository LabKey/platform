  <table tableName="Users" tableDbType="VIEW">
    <cacheSize>10000</cacheSize>
    <columns>
      <column columnName="Email">
        <columnTitle>Email</columnTitle>
        <inputType>text</inputType>
        <scale>255</scale>
        <inputLength>40</inputLength>
