{
  "name": "labkey-core",
  "version": "0.0.0",
  "scripts": {
    "clean": "bnr clean",
    "build": "npm run clean && bnr build:dev",
    "build-prod": "npm run clean && bnr build:prod",
    "prebuild": "npm run clean",
    "setup": "npm install"
  },
  "betterScripts": {
    "clean": {
      "command": "rimraf resources/web/core/css && rimraf resources/web/clientapi"
    },
    "build:dev": {
      "command": "node build.js && node webpack/buildThemes.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "node build.js && node webpack/buildThemes.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "better-npm-run": "0.1.1",
    "bootstrap-sass": "3.3.7",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "font-awesome": "4.7.0",
    "fs-extra": "7.0.1",
    "mini-css-extract-plugin": "0.4.5",
    "node-sass": "4.10.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url-loader": "1.1.2",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2"
  }
}
