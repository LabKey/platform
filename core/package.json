{
  "name": "labkey-core",
  "version": "0.0.0",
  "scripts": {
    "setup": "npm install",
    "build": "npm run clean && bnr build:dev",
    "start": "better-npm-run build:watch",
    "build-prod": "npm run clean && bnr build:prod",
    "prebuild": "npm run clean",
    "clean": "bnr clean"
  },
  "betterScripts": {
    "build:watch": {
      "command": "webpack-dev-server --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "clean": {
      "command": "rimraf resources/web/core/css && rimraf resources/web/clientapi"
    },
    "build:dev": {
      "command": "node build.js && node webpack/buildThemes.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "node build.js && node webpack/buildThemes.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-loader": "^8.0.6",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.17",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack-dev-server": "^3.9.0"
  },
  "devDependencies": {
    "@labkey/api": "0.0.22",
    "better-npm-run": "^0.1.1",
    "bootstrap-sass": "3.4.1",
    "css-loader": "3.2.0",
    "file-loader": "4.2.0",
    "font-awesome": "4.7.0",
    "fs-extra": "8.1.0",
    "mini-css-extract-plugin": "0.8.0",
    "mixin-deep": "2.0.1",
    "node-sass": "4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
